FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     variable_t_icoFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         2;

deltaT          0.000001;

adjustTimeStep  yes;

maxCo           1;

maxDeltaT       .1;

writeControl    adjustableRunTime;

writeInterval   .125;

purgeWrite      0;

writeFormat     ascii;

writePrecision  5;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


